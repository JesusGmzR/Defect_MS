{
  "name": "oqc-defectos-web",
  "version": "1.0.0",
  "description": "Sistema de registro de defectos OQC",
  "main": "dist/server/server.js",
  "scripts": {
    "start": "node dist/server/server.js",
    "dev": "nodemon --exec ts-node server/server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "start:prod": "npm run build && npm start"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.3",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
